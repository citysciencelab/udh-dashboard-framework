{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "@vue/typescript"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  }
}
